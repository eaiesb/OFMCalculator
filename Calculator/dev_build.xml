<property name="scatest.format" value="junit"/>
<property name="scatest.result" value="C:\\Go-Agent\\pipelines\\OFM-CI-PIPELINE-DEV\\Calculator"/>
<property name="scatest.partition" value="default"/>
<property name="jndi.properties.input" value="C:\\Go-Agent\\pipelines\\OFM-CI-PIPELINE-DEV\\Calculator\\dev.jndi.properties"/>
</ant>
</target>
</project>
