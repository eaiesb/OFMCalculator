	targetNamespace="http://xmlns.oracle.com/Calculator/Calculator/CalculatorBPEL"
	xmlns="http://www.w3.org/2001/XMLSchema">
	<element name="process">
		<complexType>
			<sequence>
				<element name="Number1" type="int"/>
